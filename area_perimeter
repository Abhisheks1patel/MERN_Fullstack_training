//  WAP to print area and perimeter of rectangle , taking input from the user , using prompt in js
//taking user input using prompt event handling

var l = parseInt(prompt("enter length"));
var b = parseInt(prompt("enter breadth"));

// calculation of area an perimeter
var perimeter = 2*(l+b);
var area = l*b;

// output window
console.log(area);
console.log(perimeter);

//output value

l=10,b=10

100 - perimeter
40 - area
